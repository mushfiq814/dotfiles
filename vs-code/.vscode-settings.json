		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.columnSelection": false,
	"editor.renderControlCharacters": false,
	"files.associations": {
		"*.dj": "java",
		"*.dism": "asm",
		"*.fm": "fmlang",
		"*.calc": "fmlang"
	},
	"better-comments.tags": [
		{
			"tag": "!",
			"color": "#FF2D00",
			"strikethrough": false,
			"backgroundColor": "transparent"
		},
		{
			"tag": "?",
			"color": "#3498DB",
			"strikethrough": false,
			"backgroundColor": "transparent"
		},
		{
			"tag": "//",
			"color": "#474747",
			"strikethrough": true,
			"backgroundColor": "transparent"
		},
