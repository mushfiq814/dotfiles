# modifier keys
set $mod Mod1
set $win Mod4

# Use Mouse+$mod to drag floating windows to their wanted position
floating_modifier $mod

include $HOME/.config/i3/startup
include $HOME/.config/i3/colors
include $HOME/.config/i3/layout
include $HOME/.config/i3/keybinds.general
include $HOME/.config/i3/keybinds.custom
